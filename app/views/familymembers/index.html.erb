<h1>Family Members List</h1>
  <hr class="my-4">

<table class="table table-striped table-info table-hover text-center">
  <thead class="table-dark">
    <tr>
      <th>Name</th>
      <th>Gender</th>
      <th>Age</th>
      <th>Address</th>
      <th colspan="3">Actions</th>
    </tr>
  </thead>

  <tbody>
    <% @familymembers.each do |familymember| %>
      <tr>
        <td><%= familymember.name %></td>
        <td><%= familymember.gender %></td>
        <td><%= familymember.age %></td>
        <td><%= familymember.address %></td>
        <td><%= link_to 'Show', familymember %></td>
        <td><%= link_to 'Edit', edit_familymember_path(familymember) %></td>
        <td><%= link_to 'Delete', familymember, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<hr>
<%= link_to "Add Member", new_familymember_path, class: "btn btn-danger" %>